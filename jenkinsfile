pipeline{
agent any
tools{maven 'Maven'}
stages{
  stage(checkout){
        steps{
          echo "checkout" 
        }
  }
  stage(Test){
        steps{
          echo "Test successfully" 
        }
}
stage(Build){
        steps{
           echo "Build stage complete successfully" 
        }
  }
stage(Deploy){
        steps{
            echo "Deploy stage cmplete successfully" 
        }
}
}
post{
    success{
              echo "all stages cmplete successfully" 
          }
}
}
